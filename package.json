{
  "name": "react-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/webpack.config.js --mode development",
    "build:prod": "webpack --config ./webpack/webpack.config.js --mode production",
    "test": "eslint . && jest",
    "fix": "eslint . --fix",
    "build:profile": "webpack --config ./webpack/webpack.config.js --mode production --profile --json > stats.json",
    "analyze": "webpack-bundle-analyzer ./stats.json dist"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@types/react": "^16.9.19",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.19",
    "react-test-renderer": "^16.12.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2",
    "workbox-webpack-plugin": "^5.0.0"
  },
  "dependencies": {
    "@cot/auth": "^1.0.8",
    "@cot/geosync": "^1.0.6",
    "@cot/logger": "^1.0.2",
    "@cot/session-manager": "^1.0.1",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "moduleNameMapper": {
      "^@components(.*)": "<rootDir>/src/components$1",
      "^@pages(.*)": "<rootDir>/src/pages$1",
      "^@services(.*)": "<rootDir>/src/services$1",
      "^@utils(.*)": "<rootDir>/src/utils$1",
      "^@hooks(.*)": "<rootDir>/src/hooks$1",
      "\\.module\\.css$": "identity-obj-proxy",
      "\\.(jpg|png)$": "identity-obj-proxy"
    }
  }
}
