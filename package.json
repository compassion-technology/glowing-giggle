{
  "name": "react-template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/webpack.config.js --mode development",
    "build:prod": "webpack --config ./webpack/webpack.config.js --mode production",
    "test": "eslint . && jest",
    "fix": "eslint . --fix",
    "build:profile": "webpack --config ./webpack/webpack.config.js --mode production --profile --json > stats.json",
    "analyze": "webpack-bundle-analyzer ./stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.5",
    "css-loader": "^3.6.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "prop-types": "^15.7.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpack-pwa-manifest": "^4.2.0"
  },
  "dependencies": {
    "@cot/auth": "^2.0.0",
    "@cot/logger": "^1.0.2",
    "@cot/session-manager": "^1.0.1",
    "axios": "^0.19.2",
    "identity-obj-proxy": "^3.0.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "testEnvironment": "jsdom",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "@components(.*)$": "<rootDir>/src/components/$1",
      "@pages(.*)$": "<rootDir>/src/pages/$1",
      "@services(.*)$": "<rootDir>/src/services/$1",
      "@utils(.*)$": "<rootDir>/src/utils/$1",
      "@hooks(.*)$": "<rootDir>/src/hooks/$1",
      "@assets(.*)$": "identity-obj-proxy",
      "\\.module\\.css$": "identity-obj-proxy"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "jest",
      "describe",
      "it",
      "beforeEach",
      "expect",
      "mount"
    ]
  }
}
