{
  "name": "the-inn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/webpack.dev.config.js --mode development",
    "build:prod": "webpack --config ./webpack/webpack.prod.config.js --mode production",
    "test": "eslint . && jest",
    "fix": "eslint .  --fix",
    "build:profile": "webpack --config ./webpack/webpack.prod.config.js --mode production --profile --json > stats.json",
    "analyze": "webpack-bundle-analyzer ./stats.json"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "amazon-cognito-identity-js": "^3.0.11",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^5.0.2",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-css-modules": "^2.0.0",
    "mini-css-extract-plugin": "^0.6.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.8.2",
    "react-test-renderer": "^16.8.6",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1",
    "workbox-webpack-plugin": "^4.3.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "axios": "^0.18.1",
    "core-js": "^3.2.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/test/setup.js"
    ],
    "moduleNameMapper": {
      "^@components(.*)": "<rootDir>/src/components$1",
      "^@pages(.*)": "<rootDir>/src/pages$1",
      "^@assets(.*)": "<rootDir>/src/assets$1",
      "\\.module\\.css$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "jest",
      "describe",
      "it",
      "beforeEach",
      "expect"
    ]
  }
}
